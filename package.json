{
  "name": "pertinaxwallet",
  "version": "1.0.13",
  "description": "The browser extension for Everscale blockchain.",
  "author": "pertinax wallet team <support@pertinaxwallet.com>",
  "license": "Apache-2.0",
  "scripts": {
    "lint": "eslint . --ext .svelte --ext .js --fix",
    "build": "npm run dist && gulp build",
    "dist": "rollup --config",
    "start": "rollup --config --watch",
    "test": "mocha --recursive --timeout 30000 tests/e2e/",
    "test-firstrun": "mocha --recursive --timeout 30000 tests/e2e/firstrun/*.js",
    "test-dapps": "mocha --recursive --timeout 30000 tests/e2e/dapp/*.js",
    "run-tonos": "docker run -d --name local-node -e USER_AGREEMENT=yes -p7777:80 tonlabs/local-node",
    "version": "gulp patchVersion && auto-changelog -p -b 20 -l 20 -t keepachangelog --starting-version v2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pertinaxwallet/web-extension.git"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-html": "^0.2.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.4.2",
    "auto-changelog": "^2.2.1",
    "babel-eslint": "^10.1.0",
    "chromedriver": "^87.0.4",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-mocha": "^8.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "gulp": "^4.0.2",
    "gulp-imagemin": "^7.1.0",
    "gulp-json-editor": "^2.5.4",
    "gulp-multi-process": "^1.3.1",
    "gulp-zip": "^4.0.0",
    "install": "^0.13.0",
    "mocha": "^9.1.3",
    "node-sass": "^7.0",
    "npm": "^8.3.0",
    "postcss": "^8.2.15",
    "rollup": "^2.38.5",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-styles": "^3.11.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-watch-assets": "^1.0.1",
    "selenium-webdriver": "^4.0.0-alpha.7",
    "svelte": "^3.32.3",
    "svelte-preprocess": "^4.6.9"
  },
  "dependencies": {
    "@mdi/js": "^5.9.55",
    "@tonclient/core": "^1.28.0",
    "@tonclient/lib-web": "^1.28.0",
    "bignumber.js": "^9.0.1",
    "chota": "^0.8.0",
    "eventemitter3": "^4.0.7",
    "globby": "^3.0.0",
    "idb": "^6.0.0",
    "roboto-fontface": "^0.10.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-serve": "^1.1.0",
    "svelte-chota": "^1.8.3",
    "svelte-i18n": "^3.3.6",
    "webextension-polyfill": "^0.7.0"
  }
}
